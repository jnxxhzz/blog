### Problem A 不存在的树
分析：树链剖分模板题。。
剖分后用线段树维护即可。
```cpp
#include &lt;bits/stdc++.h&gt;
using namespace std;
#define pb push_back
#define cl(a,b) memset(a,b,sizeof(a))
#define lson t&lt;&lt;1,l,mid
#define rson t&lt;&lt;1|1,mid+1,r
#define seglen(t) (node[(t)].r-node[(t)].l+1)
typedef long long LL;
const int INF = 0x3f3f3f3f;
const int maxn = 50000 + 10;
int head[maxn],cnt;
struct Edge{
    int to,next;
}edge[maxn&lt;&lt;1];
void add(int u,int v)
{
    edge[cnt].to = v;
    edge[cnt].next = head[u];
    head[u] = cnt++;
}
int num;
int val[maxn],siz[maxn],dep[maxn],son[maxn];
int top[maxn],tid[maxn],Rank[maxn],fa[maxn];
void init()
{
    cl(head,-1);cl(son,-1);
    cnt = num = 0;
}
///树链剖分部分
void dfs1(int u,int father,int d)
{
    dep[u] = d;
    fa[u] = father;
    siz[u] = 1;
    for(int i = head[u]; ~i; i = edge[i].next)
    {
        int v = edge[i].to;
        if(v != father)
        {
            dfs1(v,u,d+1);
            siz[u] += siz[v];
            if(son[u] == -1 || siz[v] &gt; siz[son[u]])
                son[u] = v;
        }
    }
}
 
void dfs2(int u,int tp)
{
    top[u] = tp;
    tid[u] = ++num;
    Rank[tid[u]] = u;
    if(son[u] == -1) return;
    dfs2(son[u],tp);
    for(int i = head[u]; ~i; i = edge[i].next)
    {
        int v = edge[i].to;
        if(v != son[u] &amp;&amp; v != fa[u])
            dfs2(v,v);
    }
}
///线段树部分
struct Node{
    int l,r,sum,mx;
}node[maxn&lt;&lt;2];
void pushup(int t)
{
    node[t].mx = max(node[t&lt;&lt;1].mx ,node[t&lt;&lt;1|1].mx);
    node[t].sum = node[t&lt;&lt;1].sum + node[t&lt;&lt;1|1].sum;
}
void build(int t,int l,int r)
{
    node[t].l = l;
    node[t].r = r;
    if(l == r){
        node[t].mx = node[t].sum = val[Rank[l]];
        return ;
    }
    int mid = (l + r) &gt;&gt; 1;
    build(lson);
    build(rson);
    pushup(t);
}
void update(int t,int u,int v)
{
    if(node[t].l == node[t].r &amp;&amp; node[t].l == u){
        node[t].sum = v;
        node[t].mx = v;
        return ;
    }
    int mid = (node[t].l + node[t].r) &gt;&gt; 1;
    if(u &lt;= mid)update(t&lt;&lt;1,u,v);
    else update(t&lt;&lt;1|1,u,v);
    pushup(t);
}
int querySum(int t,int l,int r)
{
    if(node[t].l &gt;= l &amp;&amp; node[t].r &lt;= r ){
        return node[t].sum;
    }
    int mid = (node[t].l + node[t].r) &gt;&gt; 1;
    int ret = 0;
    if(l &lt;= mid)ret += querySum(t&lt;&lt;1,l,r);
    if(r &gt; mid) ret += querySum(t&lt;&lt;1|1,l,r);
    pushup(t);
    return ret;
}
int queryMax(int t,int l,int r)
{
    if(l &lt;= node[t].l &amp;&amp; node[t].r &lt;= r){
        return node[t].mx;
    }
    int mid = (node[t].l + node[t].r) &gt;&gt; 1;
    int ret = -INF;
    if(l &lt;= mid)ret = max(ret,queryMax(t&lt;&lt;1,l,r));
    if(r &gt; mid)ret = max(ret,queryMax(t&lt;&lt;1|1,l,r));
    pushup(t);
    return ret;
}
int solveMax(int x,int y)
{
    int mx = -INF;
    while(top[x] != top[y]){
        if(dep[top[x]] &lt; dep[top[y]])swap(x,y);
        mx = max(mx,queryMax(1,tid[top[x]],tid[x]));
        x = fa[top[x]];
    }
    if(dep[x] &gt; dep[y])swap(x,y);
    return max(mx,queryMax(1,tid[x],tid[y]));
}
int solveSum(int x,int y)
{
    int sum = 0;
    while(top[x] != top[y]){
        if(dep[top[x]] &lt; dep[top[y]])swap(x,y);
        sum += querySum(1,tid[top[x]],tid[x]);
        x = fa[top[x]];
    }
    if(dep[x] &gt; dep[y])swap(x,y);
    return sum + querySum(1,tid[x],tid[y]);
}
int main()
{
    int ope;
    int n,Q;
    while(~scanf(&quot;%d%d&quot;,&amp;n,&amp;Q)){
        init();
        for( int i = 1; i &lt;= n; i++ ){
            scanf(&quot;%d&quot;,val+i);
        }
        for( int i = 0; i &lt; n-1; i++ ){
            int u,v;
            scanf(&quot;%d%d&quot;,&amp;u,&amp;v);
            add(u,v);
            add(v,u);
        }
         
        dfs1(1,0,0);
        dfs2(1,1);
        build(1,1,n);
        int u,v;
        while(Q--){
            scanf(&quot;%d%d%d&quot;,&amp;ope,&amp;u,&amp;v);
            if(ope == 0){
                printf(&quot;%d\n&quot;,solveMax(u,v));
            }
            else if(ope == 1){
                printf(&quot;%d\n&quot;,solveSum(u,v));
            }
            else {
                update(1,tid[u],v);
            }
        }
    }
    return 0;
}
 
```
### Problem B 一生之敌
分析：题目就是求存在(a,b)满足(b^2 = 2 × a × (a + 1)^2)的(b)。  
两边开根号,得:
b = (a+1) × sqrt{2a}
令(a = 2n^2),得:
b = 2n (2n^2+1) = 4n^3 + 2n
只要预处理一下(b),然后二分查询就行了。
注意要用unsigned long long处理数据。
```cpp
//#define local
#include &lt;bits/stdc++.h&gt;
using namespace std;
typedef long long LL;
typedef unsigned long long ULL;
const int maxn = 1400000 + 10;
ULL f[maxn];
void init()
{
  for (int i = 0; i &lt; maxn; i++) {
    f[i] = 4ull * i * i * i + 2ull * i;
  }
}
int main()
{
#ifdef local
  freopen(&quot;test0.in&quot;,&quot;r&quot;,stdin);
  freopen(&quot;test0.out&quot;,&quot;w&quot;,stdout);
  clock_t start_time=clock();
#endif
  init();
  int T;scanf(&quot;%d&quot;,&amp;T);
  while(T--) {
    ULL n;scanf(&quot;%llu&quot;,&amp;n);
    assert(0 &lt;= n &amp;&amp; n &lt;= 10000000000000000000ull);
    LL ans = lower_bound(f,f+maxn,n) - f;
    printf(&quot;%llu\n&quot;,f[ans]);
  }
#ifdef local
  clock_t end_time=clock();
  double run_time = (double)(end_time - start_time) / CLOCKS_PER_SEC * 1000;
  cerr&lt;&lt;&quot;Running time is : &quot;&lt;&lt;run_time&lt;&lt;&quot;ms.&quot;&lt;&lt;endl;
#endif
  return 0;
}
 
```
### Problem C 寻找ZCMU
分析：只要保存(z,c,m,u)四个字符出现的位置，枚举(z)字符的位置，其余三个字符的位置可以用二分查找。
```cpp
//#define local
#include &lt;bits/stdc++.h&gt;
using namespace std;
#define pb push_back
#define clr clear()
const int INF = 0x3f3f3f3f;
const int maxn = 100000 + 10;
char str[maxn];
vector&lt;int&gt;z,c,m,u;
int main()
{
#ifdef local
  freopen(&quot;sample.in&quot;,&quot;r&quot;,stdin);
  freopen(&quot;sample.out&quot;,&quot;w&quot;,stdout);
  clock_t start_time=clock();
#endif
  while(~scanf(&quot;%s&quot;,str)) {
    z.clr;c.clr;m.clr;u.clr;
    for (int i = 0; str[i]; i++) {
      if(str[i] == 'z') z.pb(i);
      if(str[i] == 'c') c.pb(i);
      if(str[i] == 'm') m.pb(i);
      if(str[i] == 'u') u.pb(i);
    }
    int flag = 0,ans = INF;
    for (int i = 0; i &lt; (int)z.size(); i++) {
      int p1 = upper_bound(c.begin(),c.end(),z[i]) - c.begin();
      if(p1 == c.size())continue;
      int p2 = upper_bound(m.begin(),m.end(),c[p1]) - m.begin();
      if(p2 == m.size())continue;
      int p3 = upper_bound(u.begin(),u.end(),m[p2]) - u.begin();
      if(p3 == u.size())continue;
      flag = 1;
      //cout&lt;&lt;&quot;p1 = &quot;&lt;&lt;p1&lt;&lt;&quot;p2 = &quot;&lt;&lt;p2&lt;&lt;&quot;p3 = &quot;&lt;&lt;p3&lt;&lt;endl;
      ans = min(ans,u[p3] - z[i] - 3);
    }
    printf(&quot;%d\n&quot;,flag ? ans : -1);
  }
#ifdef local
  clock_t end_time=clock();
  double run_time = (double)(end_time - start_time) / CLOCKS_PER_SEC * 1000;
  cerr&lt;&lt;&quot;Running time is : &quot;&lt;&lt;run_time&lt;&lt;&quot;ms.&quot;&lt;&lt;endl;
#endif
  return 0;
}
```
### Problem D cc的神奇背包
分析:可扩充背包，本身是个贪心，需要按a&lt;=b和a&gt;b的分类后排序，那么显然a &lt;= b的物品一定是从a从小到大排序直接放进去的，因为每放一个背包体积都会扩大，那么对另外a&gt;b的物品，显然这个时候背包的体积一直在瞎讲，那么肯定要从体积大的开始放，判断能不能全放下就可以了
```cpp
#include &lt;vector&gt;
#include &lt;stdio.h&gt;
#include &lt;string.h&gt;
#include &lt;iostream&gt;
#include &lt;algorithm&gt;
using namespace std;
inline void read(int &amp;x)
{
    x = 0; int c = getchar();
    while(c &lt; '0' || c &gt; '9') c = getchar();
    while(c &gt;= '0' &amp;&amp; c &lt;= '9') x = x * 10 + c - 48, c = getchar();
}
int n, T, v;
long long h;
struct XX{ 
    int a, b;
};
vector&lt;XX&gt; A,B;
bool cp1(const XX &amp;x, const XX &amp;y) {return x.a &lt; y.a;}
bool cp2(const XX &amp;x, const XX &amp;y) {return x.b &gt; y.b;}
bool Jud()
{
    for(int i = 0; i &lt; A.size(); ++i)
    {
        if(h &lt; A[i].a) return false;
        h += A[i].b - A[i].a;
    }
    for(int i = 0; i &lt; B.size(); ++i)
    {
        if(h &lt; B[i].a) return false;
        h += B[i].b - B[i].a;
    }
    return true;
}
int main()
{
    //freopen(&quot;4.in&quot;,&quot;r&quot;,stdin);  
    //freopen(&quot;4.out&quot;,&quot;w&quot;,stdout);  
    read(T);
    while(T--)
    {
        A.clear(), B.clear();
        read(n), read(v), h = v;
        for(int i = 0; i &lt; n; ++i)
        {
            XX x; read(x.a), read(x.b);
            if(x.a &lt; x.b) 
                A.push_back(x);
            else
                B.push_back(x);
        }
        sort(A.begin(), A.end(), cp1);
        sort(B.begin(), B.end(), cp2);
        puts(Jud() ? &quot;yes&quot;: &quot;no&quot;);
    }
     return 0;
}
```
### Problem E Pizza
分析:把一块质量为n的pizza分成等整数质量的多块，问获得最小的卡路里，那么显然最小的分块质量就是1，那么答案就是1×k，数据范围刚好比int大，使用long long即可
```cpp
#include &lt;stdio.h&gt;
int main()
{
    // freopen(&quot;4.in&quot;,&quot;r&quot;,stdin);  
    // freopen(&quot;4.out&quot;,&quot;w&quot;,stdout);  
    long long n,m;
    int pp;scanf(&quot;%d&quot;,&amp;pp);
    while (pp--)
    {
        scanf(&quot;%lld%lld&quot;,&amp;n,&amp;m);
        printf(&quot;%lld\n&quot;,m);
    }
    return 0;
}
```
### Problem F 开心的cc
分析：给出一串1和0，形成一个圈，问有多少个位置可以使得以这个点为起点，前缀和保证1的数量比0多
很有意思的证明题.....证明结果就是把0当成-1求个和...和就是答案
用反证法可以证明，在1和0个数一样的圈内比如 1 0 ，答案是0，那么对当前多出一个1的话，答案就会变成1，以此类推...
```cpp
#include &lt;stdio.h&gt;  
int main()  
{  
//  freopen(&quot;1.in&quot;,&quot;r&quot;,stdin);  
//  freopen(&quot;1.out&quot;,&quot;w&quot;,stdout);  
    int pp;scanf(&quot;%d&quot;,&amp;pp);  
    while (pp--)   
    {  
        int n;scanf(&quot;%d&quot;,&amp;n);  
        int ans=0;  
        for (int i=0;i&lt;n;i++)  
        {  
            int x;scanf(&quot;%d&quot;,&amp;x);  
            if (x==0) ans--;
            else ans++;
        }   
        if (ans&gt;0) printf(&quot;%d\n&quot;,ans);  
        else printf(&quot;0\n&quot;);  
    }  
}  
```
### Problem G 特产
分析：读取n,m  输出m-n
```cpp
#include &lt;iostream&gt;
#include &lt;cstdio&gt;
 
using namespace std;
 
int main()
{
    int T;
    double a,b;
    cin &gt;&gt; T;
    while (T--){
        scanf(&quot;%lf%lf&quot;, &amp;a, &amp;b);
        printf(&quot;%.0f\n&quot;, b - a);
    }
    return 0;
}
 
```
### Problem H 剪纸
分析:给出一张方格纸，用一条线分成两部分，且这两部分要相等，问有多少种方案，数据只到9,直接算也可以，dfs也可以，没有卡时间
```cpp
#include &lt;algorithm&gt;
#include &lt;string.h&gt;
#include &lt;iostream&gt;
#include &lt;stdio.h&gt;
#include &lt;string&gt;
#include &lt;vector&gt;
#include &lt;queue&gt;
#include &lt;map&gt;
#include &lt;set&gt;
#include &lt;ctime&gt;
using namespace std;
int ans = 0;
int mpt[20+1][20+1];
int N;
int dir[4][2] = {0,1,1,0,0,-1,-1,0};
void dfs(int x,int y)
{
    if(x == 0 || y == 0 || x == N || y == N){
        ans ++;
        return;
    }
    for(int i = 0 ; i &lt; 4 ; i ++)
    {
        int tx = x + dir[i][0];
        int ty = y + dir[i][1];
        if(mpt[tx][ty])continue;
        mpt[tx][ty] = 1;
        mpt[N-tx][N-ty] = 1;
        dfs(tx,ty);
        mpt[tx][ty] = 0;
        mpt[N-tx][N-ty] = 0;
    }
}
int main()
{
    int pp;scanf(&quot;%d&quot;,&amp;pp);
    while (pp--)
    {
        scanf(&quot;%d&quot;,&amp;N);
  if (n%2) {puts(&quot;0&quot;);continue}
        ans=0;
        memset(mpt,0,sizeof(mpt));
        mpt[N/2][N/2] = 1;
        dfs(N/2,N/2);
        printf(&quot;%d\n&quot;,ans/4);
    }
   return 0;
}
```
### Problem I Memory leak
分析：大模拟题，留了个坑，不一定只有第一行是char定义
```cpp
#include&lt;cstdio&gt;
#include&lt;cstring&gt;
#include&lt;algorithm&gt;
#include&lt;iostream&gt;
#include&lt;string&gt;
#include&lt;map&gt;
using namespace std;
const int maxn=1e5+7;
struct node{
    string s;
    string name;
    int len;
}e[maxn];
map&lt;string,int&gt;mp;
char str[maxn];
int idx;
int main(){
    int t;
    scanf(&quot;%d&quot;,&amp;t);
    getchar();
    idx=0;
    while(gets(str)!=NULL){
        if(strcmp(str,&quot;return 0;&quot;)==0){
            t--;
            if(t==0)break;
            continue;
        }
        int len=strlen(str);
        if(str[0]=='c'&amp;&amp;str[1]=='h'&amp;&amp;str[2]=='a'&amp;&amp;str[3]=='r'){
            int i=5;
            while(i&lt;len){
                string ss=&quot;&quot;;
                for(;str[i]!='[';i++)ss+=str[i];
                e[idx].name=ss;
                mp[ss]=idx;
                int x=0;
                i++;
                for(;str[i]!=']';i++){
                    x=x*10+str[i]-'0';
                }
                e[idx].len=x;
                i+=3;
                idx++;
            }
        }else if(str[0]=='g'&amp;&amp;str[1]=='e'&amp;&amp;str[2]=='t'&amp;&amp;str[3]=='s'){
            string ss=&quot;&quot;;
            int i=5;
            for(;str[i]!=' ';i++)ss+=str[i];
            int x=mp[ss];
            ss=&quot;&quot;;
            i++;
            for(;i&lt;len;i++)ss+=str[i];
            e[x].s=ss;
//            cout&lt;&lt;e[x].name&lt;&lt;&quot;end&quot;&lt;&lt;endl;
//            cout&lt;&lt;e[x].s&lt;&lt;&quot;end&quot;&lt;&lt;endl;
//            cout&lt;&lt;e[x].len&lt;&lt;endl;
//            cout&lt;&lt;endl&lt;&lt;endl;
        }else{
            int i=5;
            string ss=&quot;&quot;;
            for(;i&lt;len;i++)ss+=str[i];
            int x=mp[ss];
            int lenn=e[x].s.size();
            if(e[x].len&gt;lenn)cout&lt;&lt;e[x].s&lt;&lt;endl;
            else{
                while(x&lt;idx){
                    lenn=e[x].s.size();
//                    cout&lt;&lt;e[x].name&lt;&lt;&quot;:&quot;&lt;&lt;endl;
//                    cout&lt;&lt;e[x].s.size()&lt;&lt;&quot; &quot;&lt;&lt;len&lt;&lt;endl;
                    if(e[x].len&lt;=lenn){
                        for(int i=0;i&lt;e[x].len;i++)printf(&quot;%c&quot;,e[x].s[i]);
//                        cout&lt;&lt;endl;
                    }else{
                        cout&lt;&lt;e[x].s;
                        break;
                    }
                    x++;
                }
                cout&lt;&lt;endl;
            }
        }
    }
    return 0;
}
```
### Problem J 序列
分析：如果没有(gcd(x,y) == 1)这个限制，那么我们可以直接把所有的(b\_{a\_y})统计进一个数组里，然后将所有的(a\_{b\_x})统计进答案就行了。
有了这个限制，我们可以考虑容斥。
就是我们把所有的解都累加进答案，然后去掉(gcd(x,y) != 1)的解。
这里我们可以用莫比乌斯函数。
关于莫比乌斯函数的定义。
![](http://www.jnxxhzz.com/Public/Upload/2017-12-10/1bed72c8b56cae80ec7cd73d881c9327.png)
(p\_1,p\_2,...,p\_n)为素数。
具体可参照wiki。
接下来我们只要计算出(x,y)都为(i)的倍数时的答案，再与(u[i])相乘累加进答案就可以了。
```cpp
#include &lt;bits/stdc++.h&gt;
using namespace std;
#define fin freopen(&quot;test10.in.txt&quot;,&quot;r&quot;,stdin)
#define fout freopen(&quot;test10.out.txt&quot;,&quot;w&quot;,stdout)
#define cl(a,b) memset(a,b,sizeof(a))
typedef long long LL;
const int maxn = 100000 + 10;
bool check[maxn];
int mu[maxn],prime[maxn];
void Mobius()
{
    cl(check,false);
    mu[1] = 1;
    int tot = 0;
    for( int i = 2; i &lt; maxn; i++ ){
        if(!check[i]){
            prime[tot++] = i;
            mu[i] = -1;
        }
        for( int j = 0; j &lt; tot; j++ ){
            if(i * prime[j] &gt;= maxn)break;
            check[i*prime[j]] = true;
            if(i % prime[j] == 0){
                mu[i * prime[j]] = 0;
                break;
            }
            else {
                mu[i * prime[j]] = -mu[i];
            }
        }
    }
}
int a[maxn],b[maxn],vis[maxn],n;
LL cal(int t)
{
    LL ret = 0;
    for( int i = t; i &lt;= n; i += t )vis[b[a[i]]]++;
    for( int i = t; i &lt;= n; i += t )ret += vis[a[b[i]]];
    for( int i = t; i &lt;= n; i += t )vis[b[a[i]]]--;
    return ret;
}
int main()
{
    //fin,fout;
    Mobius();
    while(~scanf(&quot;%d&quot;,&amp;n)){
        for( int i = 1; i &lt;= n; i++ ){
            scanf(&quot;%d&quot;,a+i);
            assert(a[i] &lt;= n &amp;&amp; a[i] &gt;= 1);
        }
        for( int i = 1; i &lt;= n; i++ ){
            scanf(&quot;%d&quot;,b+i);
            assert(b[i] &lt;= n &amp;&amp; b[i] &gt;= 1);
        }
        LL ans = 0;
        for( int i = 1; i &lt;= n; i++ ){
            ans += mu[i]*cal(i);
        }
        printf(&quot;%lld\n&quot;,ans);
    }
    return 0;
}
 
```
### Problem K 追求者
分析:对于询问l~r，可以拆成1~l-1,1~r的询问。想要询问一个点到一些点的距离和，可以在重心树上直接跑。然后给出一个可持久化重心树，用与线段树类似的思路，维护当前区间的所有点数量 与到重心父亲的距离和，每加入一个节点只需要修改它在重心树上到root的距离。由于重心树深度是O(logn)，所以直接可持久化每次是O(Σ重心树上儿子数)，这显然是不优的。那么就考虑把这棵树重新建成一棵二叉树，之后的就简单了，只要注意一点，尽管转化为二叉树，重心树节点仍可以有3个儿子。可能需要RMQ O(1)求两点间距离。
```cpp
#include&lt;iostream&gt;  
#include&lt;cstdio&gt;  
#include&lt;cstring&gt;  
#include&lt;algorithm&gt;  
#define N 800005  
#define M 15000005  
#define ll long long  
using namespace std;  
     
int n,q,p,x,y,z,RT,w[N];  
int to[N],len[N],nxt[N],fst[N],u[N],l=1;  
int To[N],Len[N],Nxt[N],Fst[N],L;  
int a[N],b[N],c[N][3],cnt,st[N],ed[N],Cnt;  
int Mx[N],sz[N],fa[N];  
int ly,e[N],lg[N],pos[N];  
int rt[N],s[M][3],used[M],nd;  
     
ll r[N][26],dep[N],f[M],g[M],ans;  
     
void link(int x,int y,int z)  
{  
    to[++l]=y;len[l]=z;nxt[l]=fst[x];fst[x]=l;  
    to[++l]=x;len[l]=z;nxt[l]=fst[y];fst[y]=l;  
}  
void Link(int x,int y,int z)  
{  
    To[++L]=y;Len[L]=z;Nxt[L]=Fst[x];Fst[x]=L;  
    To[++L]=x;Len[L]=z;Nxt[L]=Fst[y];Fst[y]=L;  
}  
void edg(int x,int l,int r)  
{  
    if (l==r)  
    {  
        link(x,a[l],b[l]);  
        return;  
    }  
    int mid=l+r&gt;&gt;1,tmp=++cnt;  
         
    edg(tmp,l,mid);  
    edg(tmp,mid+1,r);  
    link(x,tmp,0);  
}  
void dfs(int x,int f)  
{  
    int t=0;  
    for (int i=Fst[x];i;i=Nxt[i])  
        if (To[i]!=f)  
            a[++t]=To[i],b[t]=Len[i];  
    if (t)  
    {  
        int mid=t+1&gt;&gt;1;   
        edg(x,1,mid);  
        if (t!=1) edg(x,mid+1,t);  
    }  
    for (int i=Fst[x];i;i=Nxt[i])  
        if (To[i]!=f)  
            dfs(To[i],x);  
}  
void Dfs(int x,int f)  
{  
    sz[x]=1;Mx[x]=0;  
    for (int i=fst[x];i;i=nxt[i])  
    {  
        if (!u[i]&amp;&amp;to[i]!=f)  
        {  
            Dfs(to[i],x);  
            sz[x]+=sz[to[i]];  
            Mx[x]=max(Mx[x],sz[to[i]]);  
        }  
    }  
}  
void Get(int x,int f,int size,int &amp;G)  
{  
    if (max(Mx[G],size-sz[G])&gt;max(Mx[x],size-sz[x])) G=x;  
    for (int i=fst[x];i;i=nxt[i])  
        if (!u[i]&amp;&amp;to[i]!=f) Get(to[i],x,size,G);  
     
}  
int build(int x)  
{  
    int G=x,sum=0;  
    Dfs(x,0);  
    if (sz[x]==1) return x;  
    Get(x,0,sz[x],G);  
    for (int i=fst[G];i;i=nxt[i])  
        if (!u[i])  
        {  
            u[i]=u[i^1]=1;  
            int t=build(to[i]);  
            c[G][sum++]=t;  
            fa[t]=G;  
        }  
    return G;  
}  
void DFS(int x,int f)  
{  
    r[pos[x]=++ly][0]=dep[x];  
    for (int i=fst[x];i;i=nxt[i])  
        if (to[i]!=f)  
        {  
            dep[to[i]]=dep[x]+len[i];  
            DFS(to[i],x);  
            r[++ly][0]=dep[x];  
        }  
}  
void DFS(int x)  
{  
    st[x]=++Cnt;  
    for (int i=0;i&lt;3;i++)  
        if (c[x][i])  
            DFS(c[x][i]);  
    ed[x]=Cnt;  
}  
ll dis(int x,int y)  
{  
    if (!x||!y) return 0;  
    x=pos[x];y=pos[y];  
    if (x&gt;y) swap(x,y);  
    int t=lg[y-x+1];  
    return r[x][0]+r[y][0]-2*min(r[x][t],r[y-e[t]+1][t]);  
}  
     
void mdy(int x,int &amp;y,int p,int q)  
{  
    if (!y) y=++nd;  
         
    g[y]=g[x]+1;  
    f[y]=f[x]+dis(fa[p],q);  
    used[y]=used[x];  
    if (p==q) used[y]=1;  
    for (int i=0;i&lt;3;i++)  
        if (c[p][i]&amp;&amp;st[q]&gt;=st[c[p][i]]&amp;&amp;st[q]&lt;=ed[c[p][i]])  
            mdy(s[x][i],s[y][i]=0,c[p][i],q);  
        else s[y][i]=s[x][i];     
}  
     
ll qry(int x,int y,int z)  
{  
    if (!x) return 0;  
    if (z==y)  
    {  
        ll ans=0;  
        for (int i=0;i&lt;3;i++)  
            if (c[y][i]) ans+=f[s[x][i]];  
        return ans;  
    }  
    ll ans=used[x]*dis(y,z);  
    for (int i=0;i&lt;3;i++)  
        if (c[y][i]&amp;&amp;st[z]&gt;=st[c[y][i]]&amp;&amp;st[z]&lt;=ed[c[y][i]])  
            ans+=qry(s[x][i],c[y][i],z);  
        else ans+=f[s[x][i]]+g[s[x][i]]*dis(y,z);  
    return ans;  
}  
     
int main()  
{  
    //reopen(&quot;4.in&quot;,&quot;r&quot;,stdin);  
    //freopen(&quot;4.out&quot;,&quot;w&quot;,stdout);  
    scanf(&quot;%d%d&quot;,&amp;n,&amp;q);  
    for (int i=1;i&lt;=n;i++)  
        scanf(&quot;%d&quot;,&amp;w[i]);  
    for (int i=2;i&lt;=n;i++)  
    {  
        scanf(&quot;%d%d%d&quot;,&amp;x,&amp;y,&amp;z);  
        Link(x,y,z);  
    }  
    cnt=n;  
    dfs(1,0);//cnt=2n  
    L=0;  
    memset(Fst,0,sizeof Fst);  
         
    RT=build(1);  
    DFS(RT);  
    DFS(1,0);//ly=4n  
         
    lg[1]=0;  
    for (int i=2;i&lt;=ly;i++)  
        lg[i]=lg[i&gt;&gt;1]+1;  
    e[0]=1;  
    for (int i=1;i&lt;=lg[ly];i++)  
        e[i]=e[i-1]&lt;&lt;1;  
    for (int i=1;i&lt;=lg[ly];i++)  
    {  
        for (int j=1;j&lt;=ly;j++)  
            r[j][i]=min(r[j][i-1],r[j+e[i-1]][i-1]);  
    }  
         
    for (int i=1,apa=0;i&lt;=n;i++)  
    {  
        mdy(rt[i-1],rt[i],RT,w[i]);//nd+=log 2n  
    }  
    while(q--)  
    {  
        scanf(&quot;%d&quot;,&amp;p);  
        if (p==1)  
        {  
            scanf(&quot;%d%d%d&quot;,&amp;x,&amp;y,&amp;z);  
            ans=qry(rt[y],RT,z)-qry(rt[x-1],RT,z);
            printf(&quot;%lld\n&quot;,ans);
        }   
        else
        {  
            scanf(&quot;%d&quot;,&amp;x); 
            mdy(rt[x-1],rt[x],RT,w[x+1]);  
            swap(w[x],w[x+1]);  
        }  
    }  
}  
```</textarea>               
